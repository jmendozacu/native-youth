<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="MagestyApps_GeoipSwitcher::geoip" title="GeoIP Store Switcher">
                        <resource id="MagestyApps_GeoipSwitcher::store_relations" title="Manage Store-Country Relations" sortOrder="10">
                            <resource id="MagestyApps_GeoipSwitcher::store_relations_save" title="Save" sortOrder="10" />
                        </resource>
                    </resource>
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="MagestyApps_GeoipSwitcher::magestyapps_geoip" title="MagestyApps Extensions &gt; GeoIP Store Switcher" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
